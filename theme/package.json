{
  "name": "t3kit",
  "version": "9.0.0",
  "description": "t3kit project. A TYPO3 website starterkit.",
  "license": "GPL-2.0",
  "authors": "t3kit Community",
  "repository": "t3kit/t3kit",
  "scripts": {
    "prod": "npm-run-all -p css-lint js-lint -s css-compile css-prefix css-minify js-compile js-minify",
    "css-compile": "node-sass --output-style=expanded --source-map=true dev/styles/bootstrap.scss  ../Resources/Public/CSS/bootstrap.css && node-sass --output-style=expanded --source-map=true dev/styles/main.scss  ../Resources/Public/CSS/main.css",
    "css-lint": "stylelint \"dev/styles/**/*.scss\" --cache --cache-location .cache/.stylelintcache",
    "css-main": "npm-run-all css-lint css-compile css-prefix css-minify",
    "css-minify": "cleancss --level=1 --source-map --source-map-inline-sources --output ../Resources/Public/CSS/bootstrap.css ../Resources/Public/CSS/bootstrap.css && cleancss --level=1 --source-map --source-map-inline-sources --output ../Resources/Public/CSS/main.css ../Resources/Public/CSS/main.css",
    "css-prefix": "postcss ../Resources/Public/CSS/bootstrap.css --use=autoprefixer --map=false --output=../Resources/Public/CSS/bootstrap.css && postcss ../Resources/Public/CSS/main.css --use=autoprefixer --map=false --output=../Resources/Public/CSS/main.css",
    "js-lint-fix": "standard --fix \"dev/js/main/**/*.js\"",
    "js-minify": "npm-run-all --parallel js-minify-bundle",
    "js-minify-bundle": "terser --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content='../Resources/Public/JavaScript/bootstrap.js.map',includeSources,url=bootstrap.min.js.map\" --output ../Resources/Public/JavaScript/bootstrap.min.js ../Resources/Public/JavaScript/bootstrap.js",
    "watch": "npm-run-all -p watch-*",
    "serve": "npm-run-all -p sync watch",
    "sync": "browser-sync start --config bs-config.js",
    "dev-css-main": "npm-run-all css-lint dev-css-compile",
    "dev-css-compile": "node-sass dev/styles/bootstrap.scss  ../Resources/Public/CSS/bootstrap.css && node-sass dev/styles/main.scss  ../Resources/Public/CSS/main.css",
    "dev-js-main": "npm-run-all js-lint dev-js-compile",
    "dev-js-compile": "rollup --config rollup.config.js",
    "watch-css": "nodemon --watch dev/styles/ --ext scss --exec  \"npm run dev-css-main\"",
    "js-compile": "rollup --config build/rollup.config.js",
    "js-main": "npm-run-all js-lint js-compile js-minify-main",
    "kk": "standard -v | snazzy",
    "done2": "browser-sync start --server 'dist'",


    "js-lint": "standard -v ./src/js/**/*.js | snazzy",
    "lint-config": "standard -v ./config/**/*.js | snazzy",
    "js-compile-development": "rollup -c config/rollup.dev.config.js",
    "js-compile-development-w": "rollup -w -c config/rollup.dev.config.js",
    "js-compile-production": "rollup -c config/rollup.prod.config.js",
    "clean-dist": "del dist/*",
    "compress-js": "npx gzipper ./dist/js",
    "compress-css": "npx gzipper ./dist/css",
    "brotli-js": "npx gzipper --brotli ./dist/js ",
    "brotli-css": "npx gzipper --brotli ./dist/css ",
    "dev": "npm-run-all js-lint js-compile-development",
    "dev1": "npm-run-all js-lint js-compile-development",
    "watch-js": "nodemon --watch src/js/ --ext js --exec \"npm run js-compile-development\"",
    "watch-jss": "dev1"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "bootstrap": "4.4.1",
    "jquery": "3.4.1",
    "popper.js": "1.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@rollup/plugin-html": "^0.1.1",
    "@rollup/plugin-inject": "^4.0.0",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "asset-rev": "^0.2.0",
    "autoprefixer": "^9.6.1",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.26.7",
    "bundlesize": "^0.18.0",
    "clean-css-cli": "^4.3.0",
    "del-cli": "^3.0.0",
    "dotenv": "^8.0.0",
    "glob": "^7.1.3",
    "globby": "^11.0.0",
    "gulp": "^4.0.2",
    "gulp-rollup": "^2.16.2",
    "gzipper": "^3.4.2",
    "hasha": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "inline-chunk-manifest-html-webpack-plugin": "^2.0.0",
    "inline-manifest-webpack-plugin": "^4.0.2",
    "node-rev": "^1.3.0",
    "node-sass": "^4.12.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "postbuild": "^2.1.0",
    "replace-in-file": "^5.0.2",
    "rev-static": "^3.4.3",
    "rollup": "^1.19.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-brotli": "^2.0.2",
    "rollup-plugin-bundle-html": "^0.2.2",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-delete": "^1.1.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-fill-html": "^1.1.0",
    "rollup-plugin-gen-html": "^0.2.6",
    "rollup-plugin-generate-html-template": "^1.6.0",
    "rollup-plugin-gzip": "^2.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "rollup-plugin-sizes": "^1.0.1",
    "rollup-plugin-static-files": "0.0.1",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-stream": "^1.24.1",
    "shx": "^0.3.2",
    "snazzy": "^8.0.0",
    "standard": "^14.3.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "terser": "^4.4.3",
    "vinyl-source-stream": "^2.0.0",
    "webpack": "v5.0.0-beta.12",
    "webpack-cli": "^3.3.10",
    "webpack-manifest-plugin": "^2.2.0",
    "yaml": "^1.7.2"
  },
  "peerDependencies": {},
  "standard": {
    "ignore": [
      "**/dist/",
      "dist/"
    ]
  }
}
