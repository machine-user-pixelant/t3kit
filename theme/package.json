{
  "name": "t3kit",
  "version": "9.0.0",
  "description": "t3kit project. A TYPO3 website starterkit.",
  "license": "GPL-2.0",
  "authors": "t3kit Community",
  "repository": "t3kit/t3kit",
  "scripts": {
    "css": "sass --watch src/styles/index.scss output.css",
    "css2": "sass --watch src/styles/index2.scss output2.css",
    "_": "_____Lint files________________________",
    "lint-lockfile": "todo",
    "lint-js": "standard -v ./src/js/**/*.js | snazzy",
    "lint-config": "standard -v ./config/**/*.js | snazzy",
    "lint-css": "stylelint \"src/styles/**/*.scss\" --cache --cache-location .cache/.stylelintcache",
    "lint-all": "npm-run-all --parallel lint-js lint-config",
    "test": "lint-all",
    "__": "_____Development________________________",
    "clean-dev-dist": "del dist/development/*",
    "js-compile-development": "rollup -c config/rollup.dev.config.js",
    "compile-all-development": "npm-run-all --parallel js-compile-development",
    "js-template": "node config/js-template.js",
    "dev": "npm-run-all lint-all clean-dev-dist compile-all-development js-template",
    "dev-test": "npm-run-all  clean-dev-dist compile-all-development js-template",
    "___": "_____Production________________________",
    "clean-prod-dist": "del dist/production/*",
    "js-compile-production": "rollup -c config/rollup.prod.config.js",
    "compile-all-production": "npm-run-all --parallel js-compile-production",
    "rev-file": "node config/rev-file.js",
    "compress-js": "npx gzipper ./dist/production/js",
    "compress-css": "npx gzipper ./dist/production/css",
    "brotli-js": "npx gzipper --brotli ./dist/production/js ",
    "brotli-css": "npx gzipper --brotli ./dist/production/css ",
    "compress": "npm-run-all --parallel compress-js brotli-js",
    "js-template-prod": "node config/js-template-prod.js",
    "prod": "npm-run-all lint-all clean-prod-dist compile-all-production rev-file compress js-template-prod",
    "prod-test": "npm-run-all clean-prod-dist compile-all-production rev-file compress js-template-prod",
    "____": "_____TYPO3 assets________________________",
    "clean-assets": "del ../Resources/Public/assets/* -f",
    "copy-assets": "cpy '**/*' ../../Resources/Public/assets/ --parents --cwd=dist",
    "assets": "npm-run-all clean-assets copy-assets",
    "_____": "_____Build dev/production files and copy TYPO3 assets_________",
    "clean-dist": "del dist/*",
    "pre-build": "npm-run-all --parallel dev prod",
    "build": "npm-run-all lint-all clean-dist pre-build assets",
    "-------": "-----Watch-------------------------",
    "js-compile-development-w": "rollup -w -c config/rollup.dev.config.js",
    "css-compile-development-w": "TBD",
    "compile-all-dev-w": "npm-run-all --parallel js-compile-development-w",
    "browsersync": "node config/br-sync.js",
    "watch": "npm-run-all --parallel compile-all-dev-w browsersync",
    "start": "npm-run-all clean-dist watch"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "bootstrap": "4.4.1",
    "jquery": "3.4.1",
    "popper.js": "1.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@rollup/plugin-html": "^0.1.1",
    "@rollup/plugin-inject": "^4.0.0",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "asset-rev": "^0.2.0",
    "autoprefixer": "^9.6.1",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.26.7",
    "bundlesize": "^0.18.0",
    "clean-css-cli": "^4.3.0",
    "copy": "^0.3.2",
    "cpy-cli": "^3.0.0",
    "del-cli": "^3.0.0",
    "domhandler": "^3.0.0",
    "dotenv": "^8.0.0",
    "glob": "^7.1.3",
    "globby": "^11.0.0",
    "gulp": "^4.0.2",
    "gulp-rollup": "^2.16.2",
    "gzipper": "^3.4.2",
    "hasha": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "htmlparser2": "^4.0.0",
    "http-server": "^0.11.1",
    "inline-chunk-manifest-html-webpack-plugin": "^2.0.0",
    "inline-manifest-webpack-plugin": "^4.0.2",
    "node-html-parser": "^1.1.19",
    "node-rev": "^1.3.0",
    "node-sass": "^4.12.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "postbuild": "^2.1.0",
    "replace-in-file": "^5.0.2",
    "rev-static": "^3.4.3",
    "rollup": "^1.19.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-brotli": "^2.0.2",
    "rollup-plugin-bundle-html": "^0.2.2",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-delete": "^1.1.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-fill-html": "^1.1.0",
    "rollup-plugin-gen-html": "^0.2.6",
    "rollup-plugin-generate-html-template": "^1.6.0",
    "rollup-plugin-gzip": "^2.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "rollup-plugin-sizes": "^1.0.1",
    "rollup-plugin-static-files": "0.0.1",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-stream": "^1.24.1",
    "sass": "^1.25.0",
    "shx": "^0.3.2",
    "simple-html-tokenizer": "^0.5.8",
    "snazzy": "^8.0.0",
    "standard": "^14.3.1",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-scss": "^3.14.2",
    "terser": "^4.4.3",
    "vinyl-source-stream": "^2.0.0",
    "webpack": "v5.0.0-beta.12",
    "webpack-cli": "^3.3.10",
    "webpack-manifest-plugin": "^2.2.0",
    "yaml": "^1.7.2"
  },
  "peerDependencies": {},
  "standard": {
    "ignore": [
      "dist/"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-recommended-scss"
  }
}
